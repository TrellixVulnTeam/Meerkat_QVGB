{
    "concurrency" : 6,
    "verification_source" : "data/misc/matt_8000_card.csv",
    "input" : {
        "hyperparameters" : "config/hyperparameters/default.json",
        "filename" : "data/input/users.csv",
        "delimiter" : ",",
        "encoding" : "utf-8"
    },
    "logging" : {
        "level" : "critical",
        "path" : "logs/foo.log",
        "formatter" : "%(asctime)s - %(name)s - %(levelname)s - %(message)s",
        "console" : true
    },
    "output" : {
        "results" : {
            "fields" : ["name", "composite.address", "factual_id", "locality", "po_box", "email"]
        },
        "file" : {
            "format" : "csv",
            "path" : "data/output/longtailLabeled.csv"
        }
    },
	"elasticsearch" : {
        "cluster_nodes" : ["brainstorm1:9200", "brainstorm2:9200"
        , "brainstorm3:9200", "brainstorm4:9200", "brainstorm5:9200", "brainstorm6:9200"
        , "brainstorm7:9200", "brainstorm8:9200", "brainstorm9:9200", "brainstorma:9200"
        , "brainstormb:9200", "localhost:9200"],
        "cache_results" : false,
        "index" : "factual_index",
        "type" : "factual_type",
        "boost_labels" : ["standard_fields"],
        "boost_vectors" :  {
            "address": [0.541],
            "address_extended": [1.282],
            "admin_region": [0.69],
            "category_labels": [1.319],
            "chain_name": [0.999],
            "email": [0.516],
            "internal_store_number": [1.627],
            "locality": [1.367],
            "name": [2.781],
            "neighborhood": [0.801],
            "po_box": [1.292],
            "post_town": [0.577],
            "postcode": [0.914],
            "region": [1.685],
            "tel": [0.597]
        }
    }
}