{
    "concurrency" : 36,
    "verification_source" : "data/misc/ground_truth_card.txt",
    "container" : "card",
    "mode" : "production",
    "input" : {
        "S3" : {
            "filter" : "2014010.*",
            "src_bucket_name" : "s3yodlee",
            "src_folders" : "ctprocessed/gpanel/",
            "src_local_path" : "/mnt/ephemeral/input/",
            "dst_bucket_name" : "yodleeproducts",
            "dst_folders" : "panels/nullcat_integration/",
            "dst_local_path" : "/mnt/ephemeral/output/",
            "error_bucket_name" : "yodleeproducts",
            "error_local_path" : "/mnt/ephemeral/output/",
            "error_s3_path" : "panels/6m/error/"
        },
        "hyperparameters" : "config/hyperparameters/default.json",
        "delimiter" : "|",
        "encoding" : "utf-8"
    },
    "logging" : {
        "level" : "warning",
        "path" : "logs/foo.log",
        "formatter" : "%(asctime)s - %(name)s - %(levelname)s - %(message)s",
        "console" : true
    },
    "output" : {
        "results" : {
            "fields" : [
                "name", "chain_id", "category_labels", "address", "locality",
                "region", "postcode", "website", "tel", "chain_name",
                "latitude", "longitude", "neighborhood", "factual_id", "internal_store_number"],
            "labels" : [
                "PHYSICAL_MERCHANT", "STORE_ID", "FACTUAL_CATEGORY", "STREET", "CITY",
                "STATE", "ZIP_CODE", "WEBSITE", "PHONE_NUMBER", "CHAIN_NAME",
                "LATITUDE", "LONGITUDE", "NEIGHBORHOOD", "FACTUAL_ID", "STORE_NUMBER"]
        },
        "file" : {
            "format" : "csv",
            "processing_location" : "/mnt/ephemeral/output",
            "delimiter" : "|"
        }
    },
    "elasticsearch" : {
        "cluster_nodes" : [
            "s01:9200",
            "s02:9200",
            "s03:9200",
            "s04:9200",
            "s05:9200",
            "s06:9200",
            "s07:9200",
            "s09:9200",
            "s10:9200",
            "s11:9200",
            "s12:9200",
            "s13:9200",
            "s14:9200",
            "s15:9200",
            "s16:9200",
            "s17:9200",
            "s18:9200"
        ],
        "index" : "factual_index_2",
        "type" : "factual_type"
    }
}
