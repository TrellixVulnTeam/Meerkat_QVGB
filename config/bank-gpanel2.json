{
    "concurrency": 20,
    "container": "__CONTAINER",
    "elasticsearch": {
        "cluster_nodes": [
            "172.31.26.85",
            "172.31.26.84",
            "172.31.26.87",
            "172.31.26.86"
        ],
        "index": "factual_index",
        "type": "factual_type"
    },
    "input": {
        "S3": {
            "filter": "__FILTER",
            "src_bucket_name": "yodleeprivate",
            "src_folders": "panels/meerkat_input/gpanel2/",
            "src_local_path": "/data/0/input/",
            "dst_bucket_name": "yodleeprivate",
            "dst_folders": "panels/meerkat_output/gpanel2/",
            "dst_local_path": "/data/1/output/",
            "dst_s3_path": "/panels/meerkat_output/gpanel2/",
            "error_bucket_name": "yodleeprivate",
            "error_local_path": "/data/1/error/",
            "error_s3_path": "/panels/meerkat/error/"
        },
        "delimiter": "|",
        "encoding": "utf-8",
        "hyperparameters": "config/hyperparameters/default.json"
    },
    "logging": {
        "console": true,
        "formatter": "%(asctime)s - %(name)s - %(levelname)s - %(message)s",
        "level": "warning",
        "path": "logs/foo.log"
    },
    "mode": "production",
    "output": {
        "file": {
            "delimiter": "|",
            "format": "csv",
            "processing_location": "/mnt/ephemeral/output"
        },
        "results": {
            "fields": [
                "name",
                "chain_id",
                "category_labels",
                "address",
                "locality",
                "region",
                "postcode",
                "website",
                "tel",
                "chain_name",
                "latitude",
                "longitude",
                "neighborhood",
                "factual_id",
                "internal_store_number"
            ],
            "labels": [
                "PHYSICAL_MERCHANT",
                "STORE_ID",
                "FACTUAL_CATEGORY",
                "STREET",
                "CITY",
                "STATE",
                "ZIP_CODE",
                "WEBSITE",
                "PHONE_NUMBER",
                "CHAIN_NAME",
                "LATITUDE",
                "LONGITUDE",
                "NEIGHBORHOOD",
                "FACTUAL_ID",
                "STORE_NUMBER"
            ]
        }
    },
    "verification_source": "data/misc/ground_truth_bank.txt"
}
